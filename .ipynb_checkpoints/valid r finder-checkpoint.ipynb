    "p = 7\n",
    "\n",
    "for r in range(1, p):\n",
    "    results = []\n",
    "    bad = False\n",
    "    for x in range(0, p-1):\n",
    "        result = pow(r, x) % p\n",
    "        if result in results:\n",
    "            bad = True\n",
    "            break\n",
    "        results.append(result)\n",
    "    \n",
    "    print(\"{}: {}\".format(r, not bad))"
   ]
